@model RenderingModel
<!--Start  - Featured Image -->
@{
    string link = "";
    if (Model.Item.Fields["link to"] != null)
    {
        link = ((Sitecore.Data.Fields.LinkField)Model.Item.Fields["link to"]).GetFriendlyUrl();
    }
}
<table border="0" cellpadding="0" cellspacing="0" width="100%">

    <tr>
        <td>
            <a href="@link" target="_blank">
                @Html.Sitecore().Field("image", Model.Item, new { @style="display: block; max-width: 686px; border:none", @w="686", @class="emailwrapto100pc", @border="0", @bc="white" })

            </a>
        </td>
    </tr>
    <tr>
        <td height="5"></td>
    </tr>
</table>

<!--End - Featured Image --> 